extends layout

block content
  h1= title
  title= title
  table(cellpadding="0", cellspacing="0", border="0")
    - for (var i = 0; i < 30; ++i) {
      tr
        - for (var j = 0; j < 30; ++j) {
          td(id= i+','+j)
        - }
    - }
  input(id="toggle", type="submit", value="Pause", onclick="toggleTicking()")
  script(src='/js/life.js')
